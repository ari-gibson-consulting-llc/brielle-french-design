---
import type { PortfolioEntryCarousels } from "../../lib/getContent";
import Carousel from "../Carousel.astro";
type Props = Pick<
  PortfolioEntryCarousels["props"],
  "firstCarousel" | "secondCarousel" | "thirdCarousel" | "fourthCarousel"
>;
const { firstCarousel, secondCarousel, thirdCarousel, fourthCarousel } =
  Astro.props;
---

{
  [firstCarousel, secondCarousel, thirdCarousel, fourthCarousel].map(
    (carousel) => carousel && <Carousel images={carousel} />
  )
}
